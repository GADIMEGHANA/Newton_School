import java.io.*; 
import java.util.*;
class Main {
    public static void main (String[] args) {
        // Your code here
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        for(int i=0;i<t;i++){
            int a=sc.nextInt();
            int b=sc.nextInt();
            int c=sc.nextInt();
            String s=((a+b==c) || (b+c==a) || (a+c==b))?"Yes":"No"; 
            System.out.println(s);
        }
    }
}
