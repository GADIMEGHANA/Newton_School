import java.io.*; 
import java.util.*;
class Main {
    public static void main (String[] args) {
        // Your code here
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        for(int i=0;i<t;i++){
            int x=sc.nextInt();
            int c=x-10;
            System.out.println(c);
        }
    }
}
